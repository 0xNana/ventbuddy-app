# Production Environment Configuration
# Copy this to .env.production and update with your production values

# Blockchain & Contract
VITE_RPC_URL=https://mainnet.infura.io/v3/YOUR_INFURA_KEY
VITE_CONTRACT_ADDRESS=0xYOUR_PRODUCTION_CONTRACT_ADDRESS
VITE_CHAIN_ID=1

# App Configuration
VITE_APP_NAME=Ventbuddy
VITE_APP_DESCRIPTION=Private social platform with encrypted content

# Supabase Configuration (Production)
VITE_SUPABASE_URL=https://your-production-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_production_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_production_service_role_key

# Encryption
VITE_ENCRYPTION_SALT=your_production_encryption_salt

# Platform Configuration
VITE_PLATFORM_FEE_RECIPIENT=0xYOUR_PRODUCTION_FEE_RECIPIENT
VITE_PLATFORM_FEE_BASISPOINTS=500

# WalletConnect (Production)
VITE_WALLETCONNECT_PROJECT_ID=your_production_walletconnect_id

# Production Contract addresses (update with your production addresses)
VITE_FHEVM_EXECUTOR_CONTRACT=0xYOUR_PRODUCTION_EXECUTOR
VITE_ACL_CONTRACT=0xYOUR_PRODUCTION_ACL
VITE_HCU_LIMIT_CONTRACT=0xYOUR_PRODUCTION_HCU_LIMIT
VITE_KMS_VERIFIER_CONTRACT=0xYOUR_PRODUCTION_KMS_VERIFIER
VITE_INPUT_VERIFIER_CONTRACT=0xYOUR_PRODUCTION_INPUT_VERIFIER
VITE_DECRYPTION_ORACLE_CONTRACT=0xYOUR_PRODUCTION_DECRYPTION_ORACLE
VITE_DECRYPTION_ADDRESS=0xYOUR_PRODUCTION_DECRYPTION_ADDRESS
VITE_INPUT_VERIFICATION_ADDRESS=0xYOUR_PRODUCTION_INPUT_VERIFICATION
VITE_GATEWAY_URL=https://gateway.mainnet.zama.ai

# FHEVM Relayer (Production)
VITE_RELAYER_URL=https://your-production-relayer.com/relayer

# Production Environment
NODE_ENV=production

# Logger Configuration (Production - Minimal Logging)
VITE_LOG_LEVEL=error
VITE_LOGGER_ENABLED=false
